/**标准16:9*/
.cashOut{height: 100%;}
.cashOut .tabBox{width: 100%;height: 100%;position: absolute;left:0;top:0;}
.cashOut .tabBox .tabTit{text-align: center;font-size: 0;margin-top: 41rpx;}
.cashOut .tabBox .tabTit .item{width: 302rpx;height: 55rpx;background: #c0c0c0;transition:background .3s,color .3s;border-radius: 9rpx 9rpx  0 0;margin: 0 17rpx;display: inline-block;font-size: 25rpx;letter-spacing: 7rpx;font-weight: bold;line-height: 55rpx;color: #fff;}
.cashOut .showClass1 .tabTit .item:nth-child(1){color: #9cd5eb;background: rgba(255,255,255,.6);box-shadow: 7rpx -3rpx 7rpx 0rpx rgba(207,213,216,.5);}
.cashOut .showClass2 .tabTit .item:nth-child(2){color: #9cd5eb;background: rgba(255,255,255,.6);box-shadow: 7rpx -3rpx 7rpx 0rpx rgba(207,213,216,.5);}
.cashOut .tabBox .tabCont{width: 690rpx;left: 27rpx; border-radius: 20rpx;padding: 21rpx 13rpx;overflow: hidden;position: absolute;top:96rpx;bottom:30rpx;box-shadow: 7rpx 3rpx 7rpx 0rpx rgba(207,213,216,.5);background: rgba(255,255,255,.6);}

.cashOut .cashListCont{height: 95%;}
.cashOut .list{display: none; }
.cashOut .list .li{width: 665rpx;height: 264rpx;margin-bottom: 30rpx;}
.cashOut .list .li .liBg{width: 100%;height: 100%;}
.cashOut .list .li .liBtn{width: 173rpx;height: 45rpx;position: absolute;right: 43rpx;bottom: 43rpx;}

.cashOut .list .li .num{position: absolute;left: 74rpx;bottom: 74rpx;color: #fff;font-size: 42rpx;font-weight: bold;}
.cashOut .list .li .num text{font-size: 144rpx;margin-right: 8rpx;font-weight: normal;}
.cashOut .showClass1 .list1{display: block;}
.cashOut .showClass2 .list2{display: block;}

