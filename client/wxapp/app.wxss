/**app.wxss**/
@import 'common/css/common.wxss';
.content{position: absolute;width:100%;left:0;top:0;height:100%;}
.article{background:#eee;}
.bg1{width:100%;height:1479rpx;position: absolute;bottom:0;left:0;pointer-events:none;}

.footer{display: flex;position: absolute;bottom: 0;left: 0;width: 100%; align-items: center;height: 120rpx;background:#fff;box-shadow:0 0  10rpx 1rpx rgba(0,0,0,.1);}
.footer .footnav{flex:1;text-align: center;color:#8a8989;font-size: 22rpx;}
.footer .footIco{width: 45rpx;height: 45rpx;display: block;margin:0 auto 10rpx;filter: grayscale(100%);}
.footer .footnav.active{color: #9dd5eb;}
.footer .footnav.active .footIco{filter: grayscale(0);}

.navigationBar .navbar{position: absolute;left:40rpx;border-radius:56rpx;border:#b7b6b6 1rpx solid;}
.navigationBar .ico {width: 76rpx;height: 56rpx;display: inline-block;text-align: center;padding-top:12rpx;}
.navigationBar .back:after{content:'';position: absolute;right:0;border:#b7b6b6 2rpx solid;height:34rpx;}
.navigationBar .ico image{width: 33rpx;height: 32rpx;}

.citychoose .van-picker__cancel,.citychoose .van-picker__confirm{color:#1aad19;font-size: 36rpx;}
.citychoose .van-picker-column__item {font-size: 22rpx;}
.citychoose .van-picker__cancel{color:#999;}

.btn{background:#65b9f5;color:#fff;font-size: 30rpx;text-align: center;border-radius:10rpx;line-height:60rpx;}
.flex{display: flex;align-items: center;justify-content:  space-between;}
.flexv{display: flex;align-items: center;justify-content:center; flex-direction: column;}

button::after{border:none;}
.btn1{width:286rpx;height:54rpx;background:none;border:none;box-shadow:none;outline:none;}
.btn1 .btn1bg{position: absolute;width:100%;height:100%;left:0;top:0;}
.btn1 .btn1txt{text-align: center;position:relative;display: block;font-size: 25rpx;color:#0b2b5d;line-height: 52rpx;letter-spacing: 5rpx;}

.arr{width: 36rpx;height: 23rpx;position: absolute;bottom: 25rpx;left: 50%;margin-left: -18rpx;}
.bord1{border:#9cd5eb 1rpx solid;border-radius:11rpx;box-shadow:rgba(0,0,0,.1) 2rpx 2rpx 10rpx 2rpx;position:relative;}
.bord1:before{background: rgba(255,255,255,.6);position: absolute;right: 1rpx;bottom: 0;left: 8rpx;top:8rpx;content:'';border-radius:11rpx;box-shadow:-3rpx -3rpx 7rpx 0rpx rgba(255,255,255,.6);}
.padm30{padding:0 30rpx;}


.modal{position: absolute;width:100%;height:100%;left:0;top:0;background:rgba(0,0,0,.7);z-index: 10;opacity: 0;pointer-events:none;transition:all .3s;}
.modal .cont{width:660rpx;height:1008rpx;position: absolute;left:50%;margin-left:-330rpx;top:50%;margin-top:-504rpx;justify-content:  space-between;padding:7rpx 0 9rpx;}
.modal .cont:after{content:'';}
.modal .cont .modalBg{width:100%;height:100%;position: absolute;left:0;top:0;}
.modal .cont .subsPic{width:633rpx;height:671rpx;position:relative;margin:0 0 0 10rpx;display:block;border-radius:33rpx 33rpx 0 0;align-self:flex-start;align-content:flex-start;}
.modal .cont .tit{font-size: 30rpx;color:#0b2b5d;font-weight:bold;text-align: center;letter-spacing:5rpx;}
.modal .cont .txt{font-size: 24rpx;color:#0b2b5d;text-align: center;line-height: 30rpx;padding-top: 16rpx;letter-spacing:5rpx;}
.modal .close{width:73rpx;height:73rpx;position: absolute;bottom:-90rpx;left:50%;margin-left:-36rpx;}
.modal .avtar{position: absolute;width: 283rpx;height: 283rpx;border-radius: 100%;left: 174rpx;top:191rpx;border: #fff 10rpx solid;}
/*.article .index{transition:all .3s;}*/
/*.rule_show .index{ filter:blur(5rpx);}*/
.modal_show .modal {pointer-events:all;opacity: 1;}

.alert{position: absolute;width:100%;height:100%;left:0;top:0;z-index: 2;background:rgba(0,0,0,.7);opacity:0;pointer-events:none;transition:opacity .3s;z-index: 5;}
.alert .contv{width:100%;left:0;position: absolute;top:50%;-webkit-transform:translate(0,-50%);}
.alert .alertbox{width:661rpx;height:185rpx;margin:0 auto;display:flex;align-items: center;justify-content:  center;}
.alert .alertbox .alertBg{width:100%;height:100%;position: absolute;left:0;top:0;}
.alert .alertbox .txt{color:#fff;font-size: 30rpx;font-weight:bold;position:relative;}
.alert .close{width:73rpx;height:73rpx;margin:30rpx auto 0;display: block;}
.alert.alertShow{opacity:1;pointer-events:all;}


/** iphoneX */

@media screen and (width:375px) and (min-height:724px),screen and (width:414px) and (min-height:808px) {
  .footer {height:160rpx;padding-bottom: 40rpx;}
}
