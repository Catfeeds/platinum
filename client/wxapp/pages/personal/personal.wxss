/**标准16:9*/
.personal .wrap{padding: 44rpx 44rpx 0 36rpx;}
.probox{padding-left: 25rpx;}
.probox .txt1{font-size: 25rxp;color: #0b2b5d;letter-spacing: 5rpx;}
.probox .line{width: 100%;height: 16rpx;background: #9cd5eb;border-radius: 16rpx;margin: 25rpx 0 21rpx;}
.probox .line .active{background: #0b2b5d;height: 100%;position: absolute;left: 0;top:0;border-radius: 16rpx 0 0 16rpx;}
.probox .line text{font-size: 20rpx;color: #0b2b5d;position: absolute;right: -20rpx;top: 36rpx;z-index: 2;}

.probox.full .active {border-radius: 16rpx;}
.probox.full text{right: 0;}
.probox.full .last{display: none;}
.probox .num{font-size: 20rpx;color: #8a8989;vertical-align: top;}
.personal .tips{font-size: 20rpx;color: #0b2b5d;padding: 14rpx 0 8rpx;letter-spacing: 5rpx;}
.personal .bord1{padding: 18rpx 0 18rpx 32rpx;margin: 14rpx 0;color: #8a8989;}
.personal .bord1 .picker,.personal .bord1 input,.personal .add{font-size: 25rpx;letter-spacing: 5rpx;line-height: 45rpx;height: 45rpx;}
.personal .select:after{content: '';position: absolute;right:28rpx;width: 0;height: 0;border-top: #9cd5eb 16rpx solid;border-left: transparent 9rpx solid;border-right: transparent 9rpx solid;top:50%;margin-top: -6rpx;pointer-events:none;}
.personal .areas .picker{display: inline-block;}
.personal .btn1{margin: 34rpx auto 70rpx;}
.personal .add{height: 155rpx;line-height: 50rpx;}

