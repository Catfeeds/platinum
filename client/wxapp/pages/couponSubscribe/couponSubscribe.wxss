/**标准16:9*/
.navigationBar .navbar{display: none;}
.couponSubscribe{}
.couponSubscribe .btns{text-align: center;padding: 38rpx 0 0;}
.couponSubscribe .btn1{display: inline-block;margin: 0 20rpx;}

.shopBox{width: 699rpx;position: absolute;left: 50%;margin-left: -350rpx;top:130rpx;bottom: 30rpx;border-radius: 18rpx 18rpx 0 0;}
.shopBox:before{border-radius:18rpx 18rpx 0 0; }
.shopBox .shopBg{width: 100%;height: 100%;position: absolute;left: 0;top:0;pointer-events:none;}
.shopBox .shopList{position: absolute;width: 100%;left: 0;top:110rpx;bottom: 70rpx;}
.shopBox .shopList .li{border-bottom: #a7d9ec 1rpx solid;padding: 46rpx 46rpx 30rpx;display: flex;align-items: center;justify-content: space-between;width: 100%;}
.shopBox .shopList .tel{width: 120rpx;flex:0 1 120rpx;font-size: 20rpx;}
.shopBox .shopList .tel .iTel{width: 23rpx;height: 23rpx;margin-right: 12rpx;display: inline-block;vertical-align: middle;}
.shopBox .shopList .shopInfo{flex:auto;color: #8a8989;}
.shopBox .shopList .shopInfo .name{font-size: 23rpx;}
.shopBox .shopList .shopInfo .add{font-size: 14rpx;line-height: 21rpx;padding-top: 12rpx;}
.shopBox .shopList .shopInfo .dist{font-size: 14rpx;line-height: 21rpx;}
.shopBox .shopTit{font-size: 25rpx;color: #0b2b5d;text-align: center;font-weight: bold;line-height: 108rpx;border-bottom: #a7d9ec 1rpx solid;}



