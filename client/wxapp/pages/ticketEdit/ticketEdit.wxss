/**标准16:9*/
.formBox{padding: 5rpx 43rpx 0 38rpx;}
.formBox .bord1{margin-top: 49rpx;padding: 30rpx 30rpx 26rpx 30rpx;}
.select:after{content: '';position: absolute;right:28rpx;width: 0;height: 0;border-top: #9cd5eb 16rpx solid;border-left: transparent 9rpx solid;border-right: transparent 9rpx solid;top:50%;margin-top: -6rpx;pointer-events:none;}


.formBox .input {color: #8a8989;}
.formBox .upImg {width: 669rpx;height: 360rpx;}
.formBox .upImg .pic{width: 486rpx;height: 260rpx;border:#9cd5eb 2rpx solid;box-shadow:rgba(0,0,0,.1) 2rpx 2rpx 10rpx 2rpx;position: absolute;
right: 24rpx;top:48rpx;}
.formBox .upImg .txt{font-size: 16rpx;color: #8a8989;position: absolute;right: 24rpx;bottom: 22rpx;}
.formBox .upImg .add{width: 44rpx;height: 44rpx;position: absolute;left: 65rpx;top:157rpx;}
.formBox .upImg .add:before{content: '';position: absolute;background: #9cd5eb;width: 100%;height: 5rpx;left: 0;top:50%;-webkit-transform:translate(0,-50%);}
.formBox .upImg .add:after{content: '';position: absolute;background: #9cd5eb;width: 5rpx;height: 100%;left: 50%;top:0;-webkit-transform:translate(-50%,0);}
.formBox .btn1{margin: 36rpx auto;}
.formBox .lab{font-size: 25rpx;color: #8a8989;padding: 58rpx 0 0 32rpx;}

